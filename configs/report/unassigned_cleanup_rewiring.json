{
  "experiment_name": "planner_demo",
  "environment": "unassigned_cleanup",
  "per_agent_cost": "euclidean",
  "cost_reduction": "max",
  "max_planning_time": 450,
  "num_runs": 30,
  "seed":0,
  "optimize": true,
  "planners": [
    {
      "name": "eitstar",
