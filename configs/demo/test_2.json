{
  "experiment_name": "planner_demo",
  "environment": "simple_2d",
  "per_agent_cost": "euclidean",
  "cost_reduction": "max",
  "max_planning_time": 400,
  "num_runs": 4,
  "seed": 1,
  "optimize": true,
  "planners": [
    {
      "name": "rrtstar",
      "type": "rrtstar",
      "options": {
        "informed_sampling": true,
        "informed_sampling_version": 0,  
        "distance_metric": "max_euclidean",
        "p_goal": 0.1,
        "p_stay": 0.0,
        "p_uniform": 0.05,
        "shortcutting": true,
        "mode_sampling": 1, 
        "locally_informed_sampling": true

      }
    }
  ]
}